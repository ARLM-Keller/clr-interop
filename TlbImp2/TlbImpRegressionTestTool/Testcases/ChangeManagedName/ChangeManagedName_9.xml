<Rules>
  <Rule Name="Rule #1" Category="Type">
    <Condition>
      <And>
        <NativeName Operator="Equal" Value="TargetType" />
      </And>
    </Condition>
    <Action Name="ChangeManagedName">
      <Parameter Key="NewName" Value="NewType1" />
    </Action>
  </Rule>
  <Rule Name="Rule #2" Category="Type">
    <Condition>
      <And>
        <TypeKind Operator="Equal" Value="Interface" />
      </And>
    </Condition>
    <Action Name="ChangeManagedName">
      <Parameter Key="NewName" Value="NewType2" />
    </Action>
  </Rule>
</Rules>